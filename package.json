{
  "name": "next-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "eslint ./ --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "intl:extract": "formatjs extract pages/**/*.ts* components/**/*.ts* hooks/**/*.ts* consts/**/*.ts* --out-file lang/en.json --id-interpolation-pattern [sha512:contenthash:base64:6] --additional-function-names $t",
    "intl:compile-en": "formatjs compile lang/en.json --ast --out-file compiled-lang/en.json",
    "intl:compile-pl": "formatjs compile lang/pl.json --ast --out-file compiled-lang/pl.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.14",
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.6.0",
    "@prisma/client": "^3.0.2",
    "@types/jsonwebtoken": "^8.5.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "beautiful-react-hooks": "^0.35.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "framer-motion": "^4.1.17",
    "http-errors": "^1.8.0",
    "immer": "^9.0.7",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.23",
    "next": "^11.1.2",
    "next-connect": "^0.10.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.7.0",
    "react-icons": "^4.2.0",
    "react-intl": "^5.23.0",
    "react-markdown": "^7.1.0",
    "react-query": "^3.24.3",
    "recharts": "^2.1.6",
    "remark-gfm": "^3.0.1",
    "superjson": "^1.7.4",
    "tinycolor2": "^1.4.2",
    "zod": "3.5.1"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.5.0",
    "@next/eslint-plugin-next": "^11.1.2",
    "@types/bcrypt": "^5.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/lodash": "^4.14.173",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-plugin-formatjs": "^10.3.12",
    "babel-plugin-superjson-next": "^0.3.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-formatjs": "^2.19.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "husky": "=4",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prisma": "^3.0.2",
    "sass": "^1.43.4",
    "typescript": "^4.2.3"
  }
}
